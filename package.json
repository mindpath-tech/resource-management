{
  "name": "rm-bot",
  "version": "0.0.0",
  "description": "",
  "author": "Mindpath",
  "license": "MIT",
  "main": "./lib/src/index.js",
  "engines": {
    "node": "^16.0.0"
  },
  "scripts": {
    "build": "tsc --build",
    "prebuild": "rm -rf ./lib",
    "prestart": "npm run migrate:run",
    "shell": "ts-node -P ./tsconfig.json",
    "start": "NODE_ENV=production node ./lib/src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "nodemon --watch ./src -e ts --exec \"npm run start:dev\"",
    "start:dev": "ts-node -P ./tsconfig.json  src/index",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "migrate:create": "ts-node -P ./tsconfig.json  ./node_modules/.bin/typeorm migration:create -n PostRefactoring",
    "migrate:generate": "ts-node -P ./tsconfig.json  ./node_modules/.bin/typeorm migration:generate -n Refactor",
    "migrate:revert": "DEBUG=typeorm:*  ts-node -P ./tsconfig.json  ./node_modules/.bin/typeorm migration:revert",
    "migrate:run": "DEBUG=typeorm:*  ts-node -P ./tsconfig.json  ./node_modules/.bin/typeorm migration:run"
  },
  "dependencies": {
    "@azure/ms-rest-js": "^2.6.1",
    "@azure/storage-blob": "^12.9.0",
    "@sendgrid/mail": "^7.6.1",
    "aws-sdk": "^2.1049.0",
    "axios": "^0.26.1",
    "botbuilder": "^4.15.0",
    "botbuilder-ai": "^4.15.0",
    "botbuilder-dialogs": "^4.15.0",
    "cron": "^1.8.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.34",
    "mysql2": "^2.3.3",
    "node-rsa": "^1.1.1",
    "psl": "^1.8.0",
    "reflect-metadata": "^0.1.13",
    "replace": "^1.2.1",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2",
    "winston": "^3.4.0",
    "winston-daily-rotate-file": "^4.6.0",
    "xlsx": "^0.18.2"
  },
  "devDependencies": {
    "@types/cron": "^1.7.3",
    "@types/dotenv": "6.1.1",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.12",
    "@types/node-rsa": "^1.1.1",
    "@types/psl": "^1.1.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "~2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "~10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "~4.5.2"
  }
}
